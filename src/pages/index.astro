---
import "../CSS/style.css";
import "../CSS/animation.css";
import Hero from "../components/hero.astro";
import Knap from "../components/knap.astro";
import Footer from "../components/footer.astro";
import Joints from "../components/joints.astro";
import Aboutsection from "../components/aboutsection.astro";
import Lunch from "../components/lunch.astro";
import Instagram from "../components/instagram.astro";
import Playlistecard from "../components/playlistecard.astro";
import Islandskeflag from "../components/islandskeflag.astro";
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <link rel="stylesheet" href="https://use.typekit.net/cfq0rxa.css" />
    <title>Astro</title>
  </head>
  <body>
    <Hero>Burger Joint</Hero>
    <main>
      <h1 id="2" class="white">Our Joint's</h1>
      <Playlistecard />
      <Joints />
      <Lunch />
      <Aboutsection />
      <Instagram />
      <Islandskeflag />
    </main>
    <Footer />
  </body>
</html>

<style>
  main {
    background-color: rgb(0, 0, 0);
    background-image: url("/img/graineffect.png");
    background-repeat: repeat;
    text-align: center;
  }
  h1 {
    margin: 2em;
  }
</style>
